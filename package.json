  },
  "dependencies": {
    "lodash": "^4.17.4",
    "strip-json-comments": "^2.0.1",
    "textlint-rule-helper": "^2.0.0"
  },
  "release": {
    "analyzeCommits": "semantic-release-tamia/analyzeCommits",
